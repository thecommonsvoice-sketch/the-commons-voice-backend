{
  "name": "relearningnode",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "engines": {
    "node": ">=14.20.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec tsx src/server.ts",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
